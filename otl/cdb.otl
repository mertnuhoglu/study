			ex: Reuse calculated values
				SELECT l.id, c.start_date,  c.end_date, (c.end_date - c.start_date) as days_diff
				FROM log l
						CROSS JOIN LATERAL (
								SELECT to_date(l.start_timestamp, 'MM/dd/YYYY'),
											 to_date(l.end_timestamp, 'MM/dd/YYYY')
						) as c(start_date, end_date);
