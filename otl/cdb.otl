		schema
			CREATE TABLE states
				("name" int, "admin" int)
			;
			INSERT INTO states
				("name", "admin")
			VALUES
				(1, 1),
				(2, 1),
				(3, 1),
				(4, 2),
				(5, 3)
			;
		code:
			SELECT s.name,s.admin 
			FROM states s
			INNER JOIN (
			SELECT ss.admin
			FROM states ss
			GROUP BY ss.admin
			HAVING COUNT(*) < 100
			) a ON a.admin = s.admin
			ORDER BY s.admin ASC;
		out
			name  admin
			4 2
			5 3
