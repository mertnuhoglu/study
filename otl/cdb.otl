						DO
						$do$
						DECLARE
							 _counter int := 0;
						BEGIN
							 WHILE _counter < 10
							 LOOP
									_counter := _counter + 1;
									RAISE NOTICE 'The counter is %', _counter;  -- coerced to text automatically
							 END LOOP;
						END
