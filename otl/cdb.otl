				ans
					opt1: NOT EXISTS (fastest) = anti-join
						SELECT ip 
						FROM   login_log l 
						WHERE  NOT EXISTS (
							 SELECT 1              -- it's mostly irrelevant what you put here
							 FROM   ip_location i
							 WHERE  l.ip = i.ip
							 );
