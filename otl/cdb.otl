				code
					SELECT c.id, o.order_id, o.date, o.amount
					FROM customers c
							CROSS JOIN LATERAL (
									SELECT id as order_id, date, amount
									FROM orders
									WHERE customer_id = c.id
									ORDER BY date DESC
									LIMIT 2
							) o
					ORDER BY c.id;
